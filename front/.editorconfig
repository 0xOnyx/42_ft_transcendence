[*]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true

[test/**/expected.css]
insert_final_newline = false

[{package.json,.travis.yml,.eslintrc.json}]
indent_style = space